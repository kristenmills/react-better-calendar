{
  "name": "react-better-calendar",
  "version": "0.0.1",
  "description": "A react calendar.",
  "main": "lib/js/Calendar.js",
  "scripts": {
    "start": "webpack-dev-server --config demo/webpack.config.dev.js",
    "build:js": "babel src/js --out-dir lib/js",
    "build:css": "postcss -c postcss.config.js -o lib/css/main.css src/css/*.css",
    "build": "npm run build:js && npm run build:css",
    "prepublish": "npm run build",
    "build:demo": "webpack --config demo/webpack.config.prod.js",
    "deploy": "npm run build:demo && gh-pages -d demo/dist",
    "lint": "eslint --ext .js --ext .jsx src demo/src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristenmills/react-calendar.git"
  },
  "keywords": [
    "react",
    "calendar"
  ],
  "author": "Kristen Mills",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kristenmills/react-calendar/issues"
  },
  "homepage": "https://github.com/kristenmills/react-calendar#readme",
  "files": [
    "lib/"
  ],
  "dependencies": {
    "moment": "^2.18.1",
    "react": "^15.5.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "gh-pages": "^0.12.0",
    "html-webpack-inline-chunk-plugin": "^1.1.1",
    "html-webpack-plugin": "^2.28.0",
    "postcss-cli": "^3.1.1",
    "postcss-loader": "^1.3.3",
    "postcss-short": "^3.0.3",
    "precss": "^1.4.0",
    "react-dom": "^15.5.4",
    "resolve-url-loader": "^2.0.2",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
