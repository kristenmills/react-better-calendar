{
  "extends": [
    "airbnb",
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
  },
  "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true
  },
  "rules": {
     "max-len": [1, 180, 4],
     "react/jsx-no-bind": [0],
     "react/no-array-index-key": [0],
     "react/forbid-prop-types": [0],
     "import/no-extraneous-dependencies": [0],
  }
}
